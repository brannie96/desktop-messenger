{
	"main": "splash.html",
	"name": "desktop-messenger",
	"description": "Simple desktop client for Facebook Messenger",
	"version": "1.0.1",
	"build_date": "2015-04-20 19:01:37",
	"single-instance": true,
	"chromium-args": "--disable-gpu",
	"window": {
		"title": "Messenger Loading",
		"icon": "assets/icon.png",
		"show": false,
		"toolbar": true,
		"frame": true,
		"width": 960,
		"height": 680,
		"position": "center",
		"always-on-top": true,
		"show_in_taskbar": false,
		"transparent": false
	},
	"maintainers": [
		{
			"name": "Vladislav Stoitsov",
			"email": "vlado@netlabs.bg",
			"web": "http://www.netlabs.bg"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/vlados/desktop-messenger.git",
			"path": "/"
		}
	],
	"webkit": {
		"page-cache": true,
		"plugin": true
	},
	"manifestUrl": "http://messenger.dev.netlabs.bg/latest/package.json",
	"packages": {
		"mac": {
			"url": "http://messenger.dev.netlabs.bg/latest/Messenger-mac32.zip"
		},
		"win": {
			"url": "http://messenger.dev.netlabs.bg/latest/Messenger-win32.zip"
		},
		"linux32": {
			"url": "http://messenger.dev.netlabs.bg/latest/Messenger-linux32.zip"
		},
		"linux64": {
			"url": "http://messenger.dev.netlabs.bg/latest/Messenger-linux64.zip"
		}
	}
}